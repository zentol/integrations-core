          "title": "Sites up",
          "title_size": "16",
          "title_align": "center",
          "type": "check_status",
          "check": "iis.site_up",
          "grouping": "cluster",
          "group": "$site",
          "group_by": [],
          "tags": ["$site"]
        },
        "layout": { "x": 5, "y": 0, "width": 3, "height": 2 }
      },
